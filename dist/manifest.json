{
  "manifest_version": 3,
  "name": "Cattlelog for Chrome",
  "description": "An easier way to pick the right classes and export them to your calendar",
  "version": "1.0.0",
  "icons": {
    "16": "public/icon128.png",
    "32": "public/icon128.png",
    "48": "public/icon128.png",
    "128": "public/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "public/icon128.png",
      "32": "public/icon128.png",
      "48": "public/icon128.png",
      "128": "public/icon128.png"
    },
    "default_popup": "src/popup/index.html"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/main.tsx-loader-CBXjjVw4.js"
      ],
      "matches": [
        "https://my.ucdavis.edu/schedulebuilder/index.cfm?*"
      ]
    }
  ],
  "permissions": [
    "storage",
    "activeTab"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://my.ucdavis.edu/*"
      ],
      "resources": [
        "assets/courseUtils-Bbv0IdLz.js",
        "assets/main.tsx-CCrZSlEn.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
